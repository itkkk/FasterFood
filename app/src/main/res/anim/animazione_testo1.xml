<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:fillAfter="true"
    android:interpolator="@android:anim/linear_interpolator" >

    <translate
        android:duration="150"
        android:fillAfter="true"
        android:fromXDelta="80%p"
        android:toXDelta="15%p"
        android:fromYDelta="68%p"
        android:toYDelta="68%p"
        android:startOffset="380"/>

    <alpha
        android:duration="30"
        android:fromAlpha="0.0"
        android:interpolator="@android:anim/accelerate_interpolator"
        android:toAlpha="1.0"
        android:startOffset="380"/>

    <translate
        android:duration="110"
        android:fillAfter="true"
        android:fromXDelta="0%p"
        android:toXDelta="10%p"
        android:startOffset="530"/>

    <translate
        android:duration="80"
        android:fillAfter="true"
        android:fromXDelta="0%p"
        android:toXDelta="-7%p"
        android:startOffset="640"/>

    <translate
        android:duration="70"
        android:fillAfter="true"
        android:fromXDelta="0%p"
        android:toXDelta="5%p"
        android:startOffset="720"/>

    <translate
        android:duration="60"
        android:fillAfter="true"
        android:fromXDelta="0%p"
        android:toXDelta="-3%p"
        android:startOffset="790"/>
</set>